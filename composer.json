{
    "name": "zendframework/zend-xmlrpc",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "xmlrpc"
    ],
    "homepage": "https://github.com/zendframework/zend-xml-rpc",
    "autoload": {
        "psr-4": {
            "Zend\\XmlRpc\\": "src/"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "zendframework/zend-http": "2.4.2",
        "zendframework/zend-math": "2.4.2",
        "zendframework/zend-server": "2.4.2",
        "zendframework/zend-stdlib": "2.4.2",
        "zendframework/zendxml": "1.*"
    },
    "suggest": {
        "zendframework/zend-cache": "To support Zend\\XmlRpc\\Server\\Cache usage"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev",
            "dev-develop": "2.5-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\XmlRpc\\": "test/"
        }
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "satooshi/php-coveralls": "dev-master",
        "phpunit/PHPUnit": "~4.0"
    }
}
